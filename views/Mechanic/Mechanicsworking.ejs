<% include ../menu.html %>
<div style="margin-left: 160px;padding:6px 8px 6px 16px;"><br><br>
<div class="page-header"><h1> Mechanics Working</h1></div>
        <div class="col-lg-9">
            <% var keys=Object.keys(agents)  %>
             <div class="card example-1 square scrollbar-dusty-grass square thin">
               <div class="card-body anyClass">

            <table class="table" border="2">
                <thead class="thead-dark">
                <tr>
                    <th scope="col">Total Online</th>
                    <th scope="col"><p><%= size %></p></th>
                </tr>
                <tr>
                    <th scope="col">UserName</th>
                    <th scope="col">Phone</th>
                    <th scope="col" >View Trip</th>
                    <th scope="col">View Location</th>
                </tr>
                </thead>
                <tbody>
                <% keys.forEach(function(agent){ %>
                    <tr onmouseover="{ viewAgentServiceLocation('<%= agent %>','mechanicsWorking') }">
                        <td><%= agents[agent].agentName %></td>
                        <td><%= agents[agent].agentPhone %></td>
                        <td><button class="btn btn-primary"onclick="{ viewAgentService('<%= agent %>','Mechanics','mechaniccustomerRequest') }">Service Details</button></td>
                        <td><button class="btn btn-primary"onclick="{ viewAgentServiceLocation('<%= agent %>','mechanicsWorking') }">Service Location</button></td>
                    </tr>
                <% })%>

                </tbody>
            </table>
        </div></div></div>

        <div class="col-lg-3" >
            <h3>Service Details</h3>
            <h5 id="destination"></h5>
            <h5 id="customer"></h5>

            <h3>Service Location</h3>
            <div id="map">
            </div>
            <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNtTpw_MrAdwrPlXGTlVVS5BjH5xZeh50&callback=initMap"
                    async defer></script>
        </div>
    </div>
</html>