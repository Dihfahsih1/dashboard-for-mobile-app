<% include ../menu.html %>
<div style="margin-left: 160px;padding:6px 8px 6px 16px;"><br><br>
     <div class="page-header"><h3> Cleaning Agents Working</h3></div>
    <div class="col-lg-9">
        
      <div class="card example-1 square scrollbar-dusty-grass square thin">
       <div class="card-body anyClass"> 
        <% var keys=Object.keys(workingcleaningAgents)  %>
        <table class="table" border="2">
            <thead class="thead-dark">
            <tr>
                <th>Total Online</th>
                <th><p><%= size %></p></th>
            </tr>
            <tr>
                <th>UserName</th>
                <th>Phone</th>
                <th>View Trip</th>
                <th>View Location</th>
            </tr>
            </thead>
            <tbody>
            <% keys.forEach(function(agent){ %>
                <tr onmouseover="{ viewCleaningServiceLocation('<%= agent %>') }">
                    <td><%= workingcleaningAgents[agent].agentName %></td>
                    <td><%= workingcleaningAgents[agent].agentPhone %></td>
                    <td><button class="btn btn-primary"onclick="{ viewCleaningService('<%= agent %>') }">Service Details</button></td>
                    <td><button class="btn btn-primary"onclick="{ viewCleaningServiceLocation('<%= agent %>') }">Service Location</button></td>
                </tr>
            <% })%>

            </tbody>
        </table>
    </div></div></div>
    <div class="col-lg-3" >
        <b>Service Details</b>
        <h5 id="destination"></h5>
        <h5 id="customer"></h5>
        <h3>Service Location</h3>
        <div id="map">
        </div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDNtTpw_MrAdwrPlXGTlVVS5BjH5xZeh50&callback=initMap"
                async defer></script>
    </div>
</div>