
<% include menu.html %>
    <div style="margin-left: 160px;padding:6px 8px 6px 16px;"><br><br>
    <div class="page-header"> <h1 class="text-center">Driver Payments</h1></div>
    <div class="card example-1">
      <div class="card-body">
        <table class="table table-hover" border="2">
                <thead class="thead-dark">
                  <tr>
                      <th>Date</th>
                    <th>Driver Name</th>
                    <th>Telephone</th>
                    <th>Number Plate</th>
                    <th>Amount</th>
                  </tr>
                </thead>
                <tbody>
              <% for(var i=0; i<obj.length; i++) {%>
                  <tr>
                      <td><%= obj[i]['date'] %></td>
                    <td><%= obj[i]['name'] %></td>
                    <td><%= obj[i]['phone'] %></td>
                    <td><%= obj[i]['numberPlate'] %></td>
                    <td><%= obj[i]['amount']  %> UGX</td>
                    
                  </tr>
              <% }%> 
                </tbody>
              </table>
     </div>
    </div>
  </div>
</div>
