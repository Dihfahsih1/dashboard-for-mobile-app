<% include menu.html %>
    <div style="margin-left: 160px;padding:6px 8px 6px 16px;"><br><br>
         <div class="page-header"><h1>App Clients' Wallet Top ups</h1></div>
         
          
            <div class="card example-1">
            <div class="card-body">
        <% var keys=Object.keys(customerTransactions) %>
        Total Transactions: <%= size %>
        <table class="table table-hover" border="1">
          <thead class="thead-dark">
                  <tr>
                    <th>Date</th>
                    <th>customerID</th>
                    <th>Remark</th>
                    <th>TransactionID</th>
                    <th>Amount</th>
                    <th>Phone</th>
                  </tr>
                </thead> 
                <tbody>
              <% keys.forEach(function(key){ %>
                  <tr>
                    <td><%= customerTransactions[key].date %></td>
                    <td><%= customerTransactions[key].customer_id %></td>
                    <td><%= customerTransactions[key].remark %></td>
                    <td><%= customerTransactions[key].transaction_id %></td>
                    <td><%= customerTransactions[key].amount %> UGX</td>
                    <td><%= customerTransactions[key].phone %></td>
                    
                  </tr>
              <% })%> 
                </tbody>
              </table>
     </div>
    </div>
  </div>
</div>